{
	"manifest_version": "ext-v1",
	"name": "TLDraw - Drawing board",
	"short_name": "TLDraw",
	"version": "1.0.5",
	"description": "TLDraw is a blank infinite canvas that lets you draw lines or objects, write text and insert media like images, videos or GIFs.",
	"author": "Pavlo Symonov",
	"homepage_url": "https://github.com/tldraw/tldraw",
	"icons": {
		"128": "icons/icon-128.png",
		"background": "icons/icon-background.png",
		"banner": "icons/icon-banner.png",
		"128-dark": "icons/icon-128-dark.png",
		"background-dark": "icons/icon-background-dark.png",
		"banner-dark": "icons/icon-banner-dark.png"
	},
	"scripts": [
		"main.js"
	],
	"modules": {
		"console": {},
		"runtime": {
			"optional": false,
			"permissions": [
					"reload"
			]
		},
		"tabs": {},
		"windows": {},
		"webviews": {},
		"websessions": {}
	}
}